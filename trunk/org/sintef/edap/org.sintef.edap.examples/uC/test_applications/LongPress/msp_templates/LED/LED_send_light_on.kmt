package edapuC;
require kermeta
require "edap_LED.kmt"
using kermeta::standard
using kermeta::utils
using edap
class LED_send_light_on inherits DeviceCodeGenerator {
method generate(d : UCDevice):String is do
var _res: StringBuffer init StringBuffer.new
_res.append("// Implemetation of void LED_send_light_on()\n\n\tswitch(led_id) {\n")
d.LEDs.each{ l | 
_res.append("\tcase ")
_res.append(l.ADDR)
_res.append(": ")
_res.append(l.PORT)
_res.append("OUT |= (1 << ")
_res.append(l.BIT)
_res.append("); // ON\n\t\tbreak;\n")
}
_res.append("\tdefault: break;\n\t}\n")
result := _res.toString
end
}
