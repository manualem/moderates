import "../../devices/msp/MSP430Sensor.edapuC"
import "../../devices/msp/Led.edapuC"
import "../../devices/msp/Timer.edapuC"

edap "TestTempSensor.edap"

application TestTempSensor {

headers <template>
#include <signal.h>
#include <stdlib.h>
#include <string.h>
</template>

init <template>
	int i;
	// Stop watchdog timer
	WDTCTL = WDTPW + WDTHOLD;
	
	// Set the clock
	BCSCTL1 = CALBC1_8MHZ;
	DCOCTL = CALDCO_8MHZ;
	BCSCTL3 |= LFXT1S_2;

</template>

poll <template>
	// some sort of polling delay
	for(i=50000; i>0; i--) __no_operation();
</template>

global <template>
</template>

}
